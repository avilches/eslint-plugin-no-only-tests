{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "eslint-plugin-no-only-tests"
    ],
    "rules": {
        "no-only-tests/no-only-tests": "error"
    }
}
